eld\n               label=\"Reply\"\n               solo\n               class=\"mr-4\"\n            ></v-text-field>\n            <v-btn\n               color=\"primary\"\n               class=\"ma-0 white--text\"\n               large\n            >\n               Send\n               <v-icon right dark>send</v-icon>\n            </v-btn>\n         </div>\n      </vue-perfect-scrollbar>\n   </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport { getCurrentAppLayout } from \"Helpers/helpers\";\n\nexport default {\n  computed: {\n    ...mapGetters([\"selectedEmail\"])\n  },\n  methods: {\n    backToEmails() {\n      this.$store.dispatch(\"backToEmails\");\n    },\n    getScrollHeight() {\n      let layout = getCurrentAppLayout(this.$router);\n      switch (layout) {\n        case \"default\":\n          return \"height:calc(100vh - 220px)\";\n        case \"mini\":\n          return \"height:calc(100vh - 220px)\";\n        case \"horizontal\":\n          return \"height:calc(100vh - 220px)\";\n        case \"boxed\":\n          return \"height:calc(100vh - 391px)\";\n        case \"boxed-v2\":\n          return \"height:calc(100vh - 391px)\";\n        default:\n          return \"height:calc(100vh - 220px)\";\n      }\n    },\n    onDeleteEmail() {\n      this.$store.dispatch(\"onDeleteEmail\");\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/inbox"}]}