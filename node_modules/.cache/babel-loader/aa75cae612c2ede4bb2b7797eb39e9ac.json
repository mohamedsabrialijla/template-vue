ion>\n\t\t\t\t\t\t</v-list-tile>\n\t\t\t\t\t\t\n\t\t\t\t\t</div>\n\t\t\t\t</template>\n\t\t\t</v-list>\n\t\t</vue-perfect-scrollbar>\n\t</div>\n</template>\n\n<script>\nimport api from \"Api\";\n\nexport default {\n  data() {\n    return {\n      loader: true,\n      supportTickets: null,\n      settings: {\n        maxScrollbarLength: 100\n      }\n    };\n  },\n  mounted() {\n    this.getTickets();\n  },\n  methods: {\n    getTickets() {\n      api\n        .get(\"vuely/supportTickets.js\")\n        .then(response => {\n          this.loader = false;\n          this.supportTickets = response.data;\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/components/Widgets"}]}